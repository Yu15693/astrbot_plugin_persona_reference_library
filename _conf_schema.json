{
  "provider": {
    "description": "供应商",
    "type": "string",
    "options": [
      "openrouter"
    ],
    "default": "openrouter",
    "hint": "当前仅支持 openrouter。"
  },
  "base_url": {
    "description": "API Base URL",
    "type": "string",
    "default": "",
    "hint": "供应商 API 基础地址，留空时将使用供应商默认地址。"
  },
  "api_key": {
    "description": "API Key",
    "type": "string",
    "default": "",
